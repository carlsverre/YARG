package yarg{  import com.carlsverre.yagf.Game;  import com.carlsverre.yagf.Key;  import com.carlsverre.yagf.KeyManager;  import com.carlsverre.yagf.SimpleTF;  import com.carlsverre.yagf.YAGF;  import flash.display.Bitmap;  import flash.display.BitmapData;  import flash.geom.Point;  import flash.display.Sprite;  public class MainMenu extends Game  {    var menuTF:SimpleTF;      	override public function Setup():void   	{  		KeyManager.AddKeyBinding( {  			"startGame":Key.ENTER  		});  					menuTF = new SimpleTF();			menuTF.Text = "Press Enter to Start";			menuTF.FontColor = 0xffffffff;			menuTF.FontSize = 25;			menuTF.Size = new Point(300, 50);			menuTF.x = 20;			menuTF.y = 50;			addChild(menuTF);			  		menuTF.Redraw();  	}  	override public function Shutdown():void   	{  		KeyManager.RemoveAllKeyBindings();  	}  	override public function Update(delta:Number):void   	{  		if (KeyManager.ActionReleased("startGame")) {  			YAGF.Instance.SwitchGame(new RunningGame());  		}  	}	  }}